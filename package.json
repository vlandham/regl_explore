{
  "name": "regl_explore",
  "version": "0.0.1",
  "description": "a starting point to explore regl with!",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js --open --live",
    "build": "browserify index.js -t es2040 | indexhtmlify > index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlandham/regl_explore.git"
  },
  "keywords": [
    "regl",
    "webgl"
  ],
  "author": "Jim Vallandingham <vlandham@gmail.com> (http://vallandingham.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vlandham/regl_explore/issues"
  },
  "homepage": "https://github.com/vlandham/regl_explore#readme",
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "budo": "^10.0.3",
    "glslify": "^6.0.2"
  },
  "dependencies": {
    "regl": "^1.3.0"
  }
}
